{
  "name": "artesaniasbucaramanga",
  "version": "1.0.0",
  "description": "This template should help get you started developing with Vue 3 in Vite. The template uses Vue 3 `<script setup>` SFCs, check out the [script setup docs](https://v3.vuejs.org/api/sfc-script-setup.html#sfc-script-setup) to learn more.",
  "main": "app.js",
  "scripts": {
    "dev": "concurrently \"npm run build\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "node --watch . --env-file .env ./app.js",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "cd client && npm run build && npm run preview",
    "start:server": "cd server && node --watch --env-file .env ./app.js",
    "build": "vite build",
    "preview": "cd client && npm run preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-vue-next": "^0.453.0",
    "mongodb": "^6.9.0",
    "mongoose": "^8.8.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "semver": "^7.6.3",
    "socket.io": "^4.8.1",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^7.6.0",
    "nodemon": "^3.0.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "vite": "^5.4.8"
  }
}
